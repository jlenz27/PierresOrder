@{
  Layout = "_Layout";
}
<h1>Add A Vendor</h1>
    <p>Add a Vendor and there description/location</p>

    <form action="/vendors" method="post">
      <label for="vendorName">Vendor Name</label>
      <input id="vendorName" name="vendorName" type="text">
      <label for="vendorDescription">Vendor Description</label>
      <input id="vendorDescription" name="vendorDescription" type="text">
      <label for="vendorLocation">Vendor Location</label>
      <input id="vendorLocation" name="vendorLocation" type="text">
      <button type="submit">Add</button>
    </form>

    <p><a href="/">Home Page</a></p>
    <p><a href="/vendors">Back To List of Vendors</a></p>