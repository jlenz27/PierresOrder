  @{
  Layout = "_Layout";
  }
  @using PierreOrder.Models;

<h3>@Model["vendor"].Name Orders:</h3>

<ol>
@foreach (Order order in @Model["order"])
{
  <li><a href='/vendors/@Model["vendor"].Id/orders/@order.Id'>@order.Title</a></li>
}
</ol>

<p><a href='/vendors/@Model["vendor"].Id/orders/new'>Add Order</a></p>
<p><a href='/vendors'>Vendors</a></p>
<p><a href='/'>Home</a></p>
